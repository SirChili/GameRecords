
//Way of printing a file into the output
// Unrelated to main.cpp, just here for memorization

#include <fstream>
#include <iostream>
#include <string>

using namespace std;

int main() {

	char chars{};
	ifstream File;
	File.open("C://Users//A00593870//Downloads//HistoricalEsportData.csv");

	while (!File.eof()) { 
		//eof stands for "End of File"

		cout << chars;
		File.get(chars);

	}

}
